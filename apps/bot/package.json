		"chalk": "^5.0.0",
		"cross-env": "^7.0.3",
		"debug": "4.1.1",
		"discord.js": "^13.6.0",
		"dotenv": "^14.3.2",
		"fastify": "^3.27.0",
		"fastify-cors": "^6.0.2",
		"node-cache": "^5.1.2",
		"prom-client": "^14.0.1"
